mysql --user $1 --password=$2 -D $3 -N $5 -e "SELECT t.fmethod, min(ff.fattribute_value), max(ff.fattribute_value), avg(ff.fattribute_value), sum(ff.fattribute_value), count(ff.fid) FROM fattribute_to_feature ff, fdata d, ftype t WHERE t.fsource = '$4' AND t.ftypeid = d.ftypeid AND d.fid = ff.fid GROUP BY t.fmethod"

